<h1>Edit Listing</h1>

  <%= form_for(:apartment, :url => {:action => 'update', :id => @apartment.id}, :html => {:multipart => true}) do |f| %>
    <%= f.file_field :avatar %>

    <%= render(:partial => 'form', :locals => {:f => f}) %>

    <%= submit_tag("Update Listing") %>
    <%= link_to 'Return to list', 'list' %>

 <% end %>

